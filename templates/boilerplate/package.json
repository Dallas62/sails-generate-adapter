{
  "name": "<%= moduleName %>",
  "version": "0.10.0",
  "description": "<%=adapterName%> adapter for Sails / Waterline",
  "main": "index.js",
  "scripts": {
    "test": "node test/integration/runner -R spec -b"
  },
  "repository": <%= repository %>,
  "keywords": [
    "<%= adapterName %>",
    "adapter",
    "sails",
    "waterline",
    "sails.js",
    "plugin",
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {},
  "devDependencies": {
    "waterline-adapter-tests": "git://github.com/balderdashy/waterline-adapter-tests.git#associations",
    "mocha": "*",
    "captains-log": "~0.11.0"
  },
  "waterlineAdapter": {
      "type": "<%= adapterName %>",
      "interfaces": [
        "semantic",
        "queryable"
      ],
      "waterlineVersion": "~0.10.0"
    }
  }
